{
  "name": "@flopflip/launchdarkly-wrapper",
  "version": "2.1.1",
  "description": "A wrapper around the LaunchDarkly client for flipflop",
  "main": "dist/@flopflip-launchdarkly-wrapper.cjs.js",
  "module": "dist/@flopflip-launchdarkly-wrapper.es.js",
  "browser": "dist/@flopflip-launchdarkly-wrapper.umd.js",
  "scripts": {
    "preversion": "npm run build",
    "prebuild": "rimraf dist/**",
    "build": "cross-env run-p build:es build:cjs",
    "build:watch": "cross-env npm run build:es -- -w",
    "build:es":
      "cross-env NODE_ENV=development BABEL_ENV=rollup rollup -c -f es -o dist/@flopflip-launchdarkly-wrapper.es.js",
    "build:cjs":
      "cross-env NODE_ENV=development BABEL_ENV=rollup rollup -c -f cjs -o dist/@flopflip-launchdarkly-wrapper.cjs.js"
  },
  "files": ["readme.md", "dist/**"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdeekens/flopflip.git"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/flopflip/issues"
  },
  "homepage": "https://github.com/tdeekens/flopflip#readme",
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1"
  },
  "dependencies": {
    "ldclient-js": "^1.1.12",
    "lodash.camelcase": "^4.3.0",
    "nanoid": "^0.2.0"
  },
  "keywords": ["feature-flags", "feature-toggles", "LaunchDarkly", "client"]
}
