{
  "name": "@flopflip/react",
  "version": "2.0.1",
  "description": "A feature toggle wrapper to use LaunchDarkly with React",
  "main": "dist/@flopflip-react.cjs.js",
  "module": "dist/@flopflip-react.es.js",
  "browser": "dist/@flopflip-react.umd.js",
  "scripts": {
    "preversion": "npm run build",
    "prebuild": "rimraf dist/**",
    "build": "cross-env run-p build:es build:cjs",
    "build:watch": "cross-env npm run build:es -- -w",
    "build:es":
      "cross-env NODE_ENV=development BABEL_ENV=rollup rollup -c -f es -o dist/@flopflip-react.es.js",
    "build:cjs":
      "cross-env NODE_ENV=development BABEL_ENV=rollup rollup -c -f cjs -o dist/@flopflip-react.cjs.js"
  },
  "files": ["readme.md", "dist/**"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdeekens/flopflip.git"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/flopflip/issues"
  },
  "homepage": "https://github.com/tdeekens/flopflip#readme",
  "devDependencies": {
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "prop-types": "15.5.10",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-test-renderer": "^15.6.1"
  },
  "peerDependencies": {
    "prop-types": "15.5.10",
    "react": "^15.6.0",
    "react-dom": "^15.6.0"
  },
  "dependencies": {
    "@flopflip/launchdarkly-wrapper": "^2.1.1",
    "lodash.intersection": "^4.4.0",
    "lodash.isnil": "^4.0.0",
    "recompose": "^0.25.0"
  },
  "keywords": [
    "react",
    "feature-flags",
    "feature-toggles",
    "LaunchDarkly",
    "client"
  ]
}
