{
  "name": "@flopflip/react",
  "version": "15.0.1",
  "description": "A feature toggle wrapper to use LaunchDarkly with React",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "browser": "./dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "dist/*.d.ts",
        "dist/*.d.cts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "check-types": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "dev": "tsup --watch --clean=false"
  },
  "files": [
    "readme.md",
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tdeekens/flopflip.git",
    "directory": "packages/react"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/flopflip/issues"
  },
  "homepage": "https://github.com/tdeekens/flopflip#readme",
  "devDependencies": {
    "@flopflip/test-utils": "workspace:*",
    "@flopflip/tsconfig": "workspace:*",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tsup": "8.5.0"
  },
  "peerDependencies": {
    "react": "18.x || 19.x",
    "react-dom": "18.x || 19.x"
  },
  "dependencies": {
    "@babel/runtime": "7.27.4",
    "@flopflip/cache": "workspace:*",
    "@flopflip/types": "workspace:*",
    "@types/react-is": "19.0.0",
    "lodash": "4.17.21",
    "react-is": "19.1.0",
    "tiny-warning": "1.0.3",
    "ts-deepmerge": "7.0.3"
  },
  "keywords": [
    "react",
    "feature-flags",
    "feature-toggles",
    "LaunchDarkly",
    "client"
  ]
}
