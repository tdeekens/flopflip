{
  "name": "@flopflip/launchdarkly-wrapper",
  "version": "2.1.5",
  "description": "A wrapper around the LaunchDarkly client for flipflop",
  "main": "dist/@flopflip-launchdarkly-wrapper.cjs.js",
  "module": "dist/@flopflip-launchdarkly-wrapper.es.js",
  "browser": "dist/@flopflip-launchdarkly-wrapper.umd.js",
  "scripts": {
    "preversion": "npm run build",
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:watch": "cross-env npm run build:es -- -w",
    "build:es": "cross-env NODE_ENV=development rollup -c -f es -o dist/@flopflip-launchdarkly-wrapper.es.js",
    "build:cjs": "cross-env NODE_ENV=development rollup -c -f cjs -o dist/@flopflip-launchdarkly-wrapper.cjs.js"
  },
  "files": [
    "readme.md",
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdeekens/flopflip.git"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/flopflip/issues"
  },
  "homepage": "https://github.com/tdeekens/flopflip#readme",
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "enzyme-to-json": "^3.1.1"
  },
  "dependencies": {
    "ldclient-js": "^1.1.12",
    "lodash.camelcase": "^4.3.0",
    "nanoid": "^0.2.1"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "LaunchDarkly",
    "client"
  ]
}
