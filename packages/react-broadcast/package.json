{
  "name": "@flopflip/react-broadcast",
  "version": "2.1.5",
  "description": "A feature toggle wrapper to use LaunchDarkly with React",
  "main": "dist/@flopflip-react-broadcast.cjs.js",
  "module": "dist/@flopflip-react-broadcast.es.js",
  "browser": "dist/@flopflip-react-broadcast.umd.js",
  "scripts": {
    "preversion": "npm run build",
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:cjs",
    "build:watch": "cross-env npm run build:es -- -w",
    "build:umd": "cross-env NODE_ENV=development rollup -c -f umd -o dist/@flopflip-react-broadcast.umd.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -f umd -o dist/@flopflip-react-broadcast.umd.min.js",
    "build:es": "cross-env NODE_ENV=development rollup -c -f es -o dist/@flopflip-react-broadcast.es.js",
    "build:cjs": "cross-env NODE_ENV=development rollup -c -f cjs -o dist/@flopflip-react-broadcast.cjs.js"
  },
  "files": [
    "readme.md",
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdeekens/flopflip.git"
  },
  "author": "Tobias Deekens <nerd@tdeekens.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdeekens/flopflip/issues"
  },
  "homepage": "https://github.com/tdeekens/flopflip#readme",
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "enzyme-to-json": "^3.1.1",
    "prop-types": "15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1"
  },
  "peerDependencies": {
    "prop-types": "15.6.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0"
  },
  "dependencies": {
    "@flopflip/launchdarkly-wrapper": "^2.1.5",
    "@flopflip/react": "^2.1.4",
    "lodash.isobject": "^3.0.2",
    "react-broadcast": "^0.5.2",
    "recompose": "^0.25.1"
  },
  "keywords": [
    "react",
    "feature-flags",
    "feature-toggles",
    "LaunchDarkly",
    "client"
  ]
}
